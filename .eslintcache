Data} loading={uiStateUsers === IN_PROGRESS} />\r\n      </div>\r\n      {uiStateUsers === FAILED && <Alert message={error.message} type=\"error\" showIcon />}\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default AllusersContainer;\r\n","C:\\Users\\Muhammad Salman Asif\\Desktop\\america job\\weather-dashboard\\src\\redux\\actions\\usersAction.js",[],"C:\\Users\\Muhammad Salman Asif\\Desktop\\america job\\weather-dashboard\\src\\redux\\sagas\\usersWatcher.js",["239"],"import { takeLatest, put, call, take } from \"redux-saga/effects\";\r\nimport { eventChannel } from \"redux-saga\";\r\nimport _ from \"lodash\";\r\n\r\n//firebase instance\r\nimport firebase from \"FirebaseConfig/config\";\r\n\r\n//actions\r\nimport Actions from \"redux/actions\";\r\n\r\n//constants\r\nimport {\r\n  //listening all users\r\n  LISTENER_ALL_USERS_IN_PROGRESS,\r\n  LISTENER_ALL_USERS_CANCEL,\r\n} from \"constants/actions\";\r\n\r\nconst {\r\n  //listening users\r\n  listeningAllUsersSuccess,\r\n  listeningAllUsersFailed,\r\n} = Actions;\r\n\r\nconst db = firebase.firestore();\r\nlet channel;\r\n\r\nfunction usersEventChannel() {\r\n  const listener = new eventChannel((emit) => {\r\n    const subscriber = db.collection(\"users\").onSnapshot(\r\n      (QuerySnapshot) => {\r\n        const arr = QuerySnapshot.docs.map((el) => el.data());\r\n        emit(arr);\r\n      },\r\n      (err) => emit(err)\r\n    );\r\n    return () => {\r\n      subscriber();\r\n    };\r\n  });\r\n  return listener;\r\n}\r\n\r\nfunction* listeningUsers() {\r\n  try {\r\n    channel = yield call(usersEventChannel);\r\n    while (true) {\r\n      const allUsers = yield take(channel);\r\n      yield put(listeningAllUsersSuccess(allUsers));\r\n    }\r\n  } catch (err) {\r\n    console.log(err);\r\n    yield put(listeningAllUsersFailed(err));\r\n  }\r\n}\r\n\r\nfunction* cancelListeningUsers() {\r\n  try {\r\n    yield channel.close();\r\n  } catch (err) {\r\n    console.log(err);\r\n  }\r\n}\r\n\r\nfunction* usersWatcher() {\r\n  yield takeLatest(LISTENER_ALL_USERS_IN_PROGRESS, listeningUsers);\r\n  yield takeLatest(LISTENER_ALL_USERS_CANCEL, cancelListeningUsers);\r\n}\r\nexport default usersWatcher;\r\n","C:\\Users\\Muhammad Salman Asif\\Desktop\\america job\\weather-dashboard\\src\\redux\\reducers\\users.js",[],{"ruleId":"240","replacedBy":"241"},{"ruleId":"242","replacedBy":"243"},{"ruleId":"244","severity":1,"message":"245","line":11,"column":8,"nodeType":"246","messageId":"247","endLine":11,"endColumn":15},{"ruleId":"240","replacedBy":"248"},{"ruleId":"242","replacedBy":"249"},{"ruleId":"244","severity":1,"message":"250","line":6,"column":32,"nodeType":"246","messageId":"247","endLine":6,"endColumn":39},{"ruleId":"244","severity":1,"message":"251","line":13,"column":8,"nodeType":"246","messageId":"247","endLine":13,"endColumn":9},{"ruleId":"252","severity":1,"message":"253","line":70,"column":6,"nodeType":"254","endLine":70,"endColumn":8,"suggestions":"255"},{"ruleId":"244","severity":1,"message":"256","line":9,"column":10,"nodeType":"246","messageId":"247","endLine":9,"endColumn":14},{"ruleId":"244","severity":1,"message":"257","line":20,"column":23,"nodeType":"246","messageId":"247","endLine":20,"endColumn":30},{"ruleId":"244","severity":1,"message":"258","line":90,"column":13,"nodeType":"246","messageId":"247","endLine":90,"endColumn":15},{"ruleId":"244","severity":1,"message":"258","line":95,"column":13,"nodeType":"246","messageId":"247","endLine":95,"endColumn":15},{"ruleId":"252","severity":1,"message":"259","line":39,"column":6,"nodeType":"254","endLine":39,"endColumn":20,"suggestions":"260"},{"ruleId":"244","severity":1,"message":"251","line":2,"column":8,"nodeType":"246","messageId":"247","endLine":2,"endColumn":9},{"ruleId":"244","severity":1,"message":"251","line":2,"column":8,"nodeType":"246","messageId":"247","endLine":2,"endColumn":9},{"ruleId":"244","severity":1,"message":"261","line":2,"column":10,"nodeType":"246","messageId":"247","endLine":2,"endColumn":21},{"