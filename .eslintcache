r) checkAuth(user);\r\n      else AuthFailed(\"not valid user\");\r\n    });\r\n    return () => {\r\n      console.log(\"offline\");\r\n      AuthObserver();\r\n    };\r\n  }, []);\r\n  return (\r\n    <Async\r\n      uiState={uiStateAuth}\r\n      error={error}\r\n      onSuccess={() => <ModuleRoute />}\r\n      onFailure={() => <Redirect to=\"/auth/login\" />}\r\n    />\r\n  );\r\n};\r\nconst mapStateToProps = (state) => {\r\n  return {\r\n    Auth: state.Auth,\r\n  };\r\n};\r\nconst mapDispatchToProps = (dispatch) => {\r\n  return {\r\n    checkAuth: (user) => dispatch(checkAuthInProgress(user)),\r\n    AuthFailed: (err) => dispatch(checkAuthFailed(err)),\r\n  };\r\n};\r\n\r\nexport default connect(mapStateToProps, mapDispatchToProps)(PrivateRoute);\r\n","C:\\Users\\Muhammad Salman Asif\\Desktop\\america job\\weather-dashboard\\src\\Components\\Shared\\Async\\Async.jsx",[],"C:\\Users\\Muhammad Salman Asif\\Desktop\\america job\\weather-dashboard\\src\\Components\\Shared\\Loader\\Loader.jsx",[],"C:\\Users\\Muhammad Salman Asif\\Desktop\\america job\\weather-dashboard\\src\\modules\\ModuleRoute.jsx",[],"C:\\Users\\Muhammad Salman Asif\\Desktop\\america job\\weather-dashboard\\src\\constants\\role.js",[],"C:\\Users\\Muhammad Salman Asif\\Desktop\\america job\\weather-dashboard\\src\\modules\\Admin\\AdminRouter.jsx",[],"C:\\Users\\Muhammad Salman Asif\\Desktop\\america job\\weather-dashboard\\src\\modules\\User\\UserRouter.jsx",[],"C:\\Users\\Muhammad Salman Asif\\Desktop\\america job\\weather-dashboard\\src\\modules\\Admin\\views\\MainView\\MainView.jsx",[],"C:\\Users\\Muhammad Salman Asif\\Desktop\\america job\\weather-dashboard\\src\\modules\\User\\views\\MainView\\MainView.jsx",[],"C:\\Users\\Muhammad Salman Asif\\Desktop\\america job\\weather-dashboard\\src\\pages\\AuthContainer\\AuthContainer.jsx",[],"C:\\Users\\Muhammad Salman Asif\\Desktop\\america job\\weather-dashboard\\src\\Components\\Login\\Login.jsx",[],"C:\\Users\\Muhammad Salman Asif\\Desktop\\americ